<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erdon.dao.OrderItemDao">

    <insert id="saveOrderItem">
    insert into t_order_item(`name`,`count`,`price`,`total_price`,`order_id`) value(#{name},#{count},#{price},#{total_price},#{order_id})
    </insert>
</mapper>
