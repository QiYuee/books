<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erdon.dao.OrderDao">

    <insert id="saveOrder">
     insert into t_order(order_id,create_time,price,status,user_id) value(#{order_id},#{create_time},#{price},#{status},#{user_id})
    </insert>
</mapper>
